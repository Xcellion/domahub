<script type="text/javascript">
  var message = <% if(typeof message != "undefined"){ %><%-JSON.stringify(message)%><% }else{ %>""<% } %>;
</script>
<div id="error-notification" class="is-hidden is-danger notification is-popup">
  <button class="delete"></button>
  <p id="error-message" class='is-white'></p>
</div>
<div id="success-notification" class="is-hidden is-primary notification is-popup">
  <button class="delete"></button>
  <p id="success-message" class='is-white'></p>
</div>
<script src="/js/routes/profile/notifications_logic.js" type="text/javascript"></script>
