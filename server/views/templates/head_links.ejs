</div>
</noscript>
<!-- Google Analytics -->
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-92778659-1', 'auto');
  //for GA premium custom URLs
  if (window.location.hostname != "domahub.com"){
    ga('set', 'page', "/listing/" + window.location.hostname);
  }
  else if (typeof user != "undefined" && user.email == "general@domahub.com" && user.username == "DomaHubDemo"){
    ga('set', 'page', "/demo");
  }
  //for GA custom dimension tracking
  if (typeof listing_info != "undefined"){
    ga('set', 'dimension1', listing_info.owner_id);
  }
  //for GA custom user tracking
  if (typeof user != "undefined"){
    ga('set', 'dimension2', user);
  }
  ga('send', 'pageview');
</script>
<% if (typeof listing_info != "undefined" && listing_info.ga_tracking_id) { %>
  <script>
    //Custom GA Tracking ID
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', "<%= listing_info.ga_tracking_id %>");
  </script>
<% } %>
