								</div>
								<div class="checkbox-wrapper">
									<input class="cat-checkbox" id="cb4" type="checkbox" />
									<label class="has-border" tabindex="13" for="cb4">Promotion</label>
								</div>
								<div class="checkbox-wrapper">
									<input class="cat-checkbox" id="cb5" type="checkbox" />
									<label class="has-border" tabindex="14" for="cb5">Industry</label>
								</div>
								<div class="checkbox-wrapper">
									<input class="cat-checkbox" id="cb6" type="checkbox" />
									<label class="has-border" tabindex="15" for="cb6">Start-Up</label>
								</div>
							</div>
						</div>
						<div class="control is-horizontal">
							<div class="control-label is-padded">
								<label class="label is-medium">Price / Rate</label>
							</div>
							<div class="control">
								<span class="select is-medium">
							    <select>
							      <option disabled>Select Rate</option>
							      <option>Hourly</option>
										<option>Daily</option>
										<option>Weekly</option>
										<option>Monthly</option>
										<option>Yearly</option>
							    </select>
							  </span>
							</div>
						</div>
						<div id="slider">

						</div>
						<div id="linebreak">

						</div>
					</div>
				</div>
			</div>
		</section>

		<section class="is-small">
			<div class="container">
				<
				<table class="table">
