<% if (listing_info.date_registered && (!listing_info.premium || (listing_info.premium && listing_info.domain_age))) { %>
  <div id="domain-age-module" class='level-item'>
    <small class="heading is-accent">
      Registered
    </small>
    <p id="date_registered" class="regular-font">
    </p>
  </div>
  <div id="domain-registrar-module" class='level-item'>
    <small class="heading is-accent">
      Registrar
    </small>
    <p id="registrar" class="regular-font">
      <%= listing_info.registrar %>
    </p>
  </div>
<% } %>
