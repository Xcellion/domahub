<link href="https://fonts.googleapis.com/css?family=Ubuntu:700" rel="stylesheet">
<style>
#domahub-button {
    align-items:flex-end;
    bottom:20px;
    display:flex;
    flex-direction:column;
    position:fixed;
    right:20px;
    z-index:9999999999999;
    background-color:#3cbc8d;
    border-radius:100%;
    box-shadow:0 3px 4px 0 rgba(0,0,0,.14),0 -2px 5px -3px rgba(0,0,0,.2),0 3px 5px 0 rgba(0,0,0,.12);
    box-sizing:border-box;
    color:#fff;
    font-size:24px;
    font-family:'Ubuntu',helvetica,sans-serif;
    line-height:18px;
    padding:16px;
    height:50px;
    width:50px;
    text-decoration:none;
    transition:all 0.25s ease;
    cursor:pointer;
}
#domahub-button:hover,
#domahub-button:focus {
    background-color:#fff;
    color:#3cbc8d;
}
