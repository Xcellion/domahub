        <div class="menu-info">
          <a class="dh-logo" title="Home" href="/">
            <div class="flat-logo is-hidden-laptop"></div>
            <div class="circle-logo is-hidden-widescreen"></div>
          </a>
          <p class="break-all is-hidden-laptop"><%= user.username %></p>
        </div>
        <ul class="menu-list">
          <p class="heading" style="padding-left:20px;padding-bottom:10px;">
            Steps
          </p>
          <li class="menu-item">
            <a class="step-link" data-step="2">
              <p>
                Personal Information
              </p>
              <div class="step-tracker is-hidden" data-tracker="1">
                1 of 2
              </div>
              <div class="icon is-primary is-hidden">
                <i class="fal fa-check"></i>
              </div>
              <div class="icon is-danger is-hidden">
                <i class="fal fa-minus"></i>
              </div>
            </a>
          </li>
          <li class="menu-item">
            <a class="step-link" data-step="4">
              <p>
                Payout Methods
              </p>
              <div class="step-tracker is-hidden" data-tracker="2">
                1 of 2
              </div>
              <div class="icon is-primary is-hidden">
                <i class="fal fa-check"></i>
              </div>
              <div class="icon is-danger is-hidden">
                <i class="fal fa-minus"></i>
              </div>
            </a>
          </li>
          <li class="menu-item">
            <a class="step-link" data-step="7">
              <p>
                Upload Domains
              </p>
              <div class="step-tracker is-hidden" data-tracker="3">
                1 of 2
              </div>
              <div class="icon is-primary is-hidden">
                <i class="fal fa-check"></i>
              </div>
              <div class="icon is-danger is-hidden">
                <i class="fal fa-minus"></i>
              </div>
            </a>
          </li>
          <% if (!(user.stripe_subscription_id && user.stripe_subscription)) { %>
          <li class="menu-item">
            <a class="step-link" data-step="11">
              <p>
                Complete Setup
              </p>
              <div class="icon is-primary is-hidden">
                <i class="fal fa-check"></i>
              </div>
              <div class="icon is-danger is-hidden">
                <i class="fal fa-minus"></i>
              </div>
            </a>
          </li>
          <% } %>
          <li class="menu-item">
            <a class="step-link" href="/logout">Log Out</a>
          </li>
        </ul>
      </div>

      <div class="right-content">
        <progress class="progress is-primary" value="0" max="100"></progress>
        <button class="button steps-menu-button is-stylish is-small">
          <div class="icon is-small">
            <i class="fal fa-bars"></i>
