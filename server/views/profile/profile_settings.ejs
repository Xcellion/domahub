<!DOCTYPE html>

<html class="prevent-scroll-jump">

<head>
	<title>Account Settings - DomaHub</title>
	<link rel="stylesheet" href="/css/min/profile_settings.min.css" type="text/css" />
	<%- include("../templates/head_links"); %>
</head>

<body>

  <section class="section is-fullscreen">
		<div class="wrapper">

			<%- include("profile_navbar.ejs"); %>

			<div class="right-content">

				<div class="right-content-items">
					<div class="content">
            <div class="control is-spaced">
              <div class="control-left">
                <h2 id="current-view-name">Account Settings</h2>
              </div>
              <div class="control-right">
                <div class="control is-grouped">
                    <div class="control">
                      <button id="account-submit" type="submit" form="account-details-form" class="is-flexible toolbar-submit-button toolbar-button is-hidden button is-primary is-stylish is-small">
                        <div class="icon is-small">
                          <i class="fal fa-check-circle"></i>
                        </div>
                        <span>Save</span>
                      </button>
                      <button id="stripe-account-submit" type="submit" form="stripe-personal-form" class="is-flexible toolbar-button is-hidden button is-primary is-stylish is-small">
                        <div class="icon is-small">
                          <i class="fal fa-check-circle"></i>
                        </div>
                        <span>Save</span>
                      </button>
                      <button id="payments-submit" type="submit" form="payments-form" class="is-flexible toolbar-submit-button toolbar-button is-hidden button is-primary is-stylish is-small">
                        <div class="icon is-small">
                          <i class="fal fa-check-circle"></i>
                        </div>
                        <span>Save</span>
                      </button>
                      <button id="currency-submit" type="submit" form="currency-form" class="is-flexible toolbar-submit-button toolbar-button is-hidden button is-primary is-stylish is-small">
                        <div class="icon is-small">
                          <i class="fal fa-check-circle"></i>
                        </div>
                        <span>Save</span>
                      </button>
                    </div>
                    <div class="control">
                      <a id="cancel-button" class="is-flexible toolbar-button is-hidden button is-danger is-stylish is-small">
                        <div class="icon is-small">
                          <i class="fal fa-ban"></i>
                        </div>
                        <span>Cancel</span>
                      </a>
                    </div>
                  </div>
              </div>
            </div>
					</div>
	        <div id="settings-toolbar" class="content">
	          <div class="tabs is-primary">
	            <ul>
	              <li id="account-tab" class="tab is-active">
	                <a tabindex="0" title="Account">
	                  <span>Account</span>
	                </a>
	              </li>
	              <li id="premium-tab" class="tab">
	                <a tabindex="0" title="Premium Status">
                    <span class="is-hidden-tablet">Premium</span>
	                  <span class="is-hidden-mobile">
	                    <% if (user.stripe_subscription_id) { %>
	                      Premium Status
	                    <% } else { %>
	                      Upgrade to Premium
	                    <% } %>
	                  </span>
	                </a>
	              </li>
	              <li id="payment-tab" class="tab">
	                <a tabindex="0" title="Payments">
	                  <span class="is-relative">
                      <span>Payment</span>
											<div class="stripe-required-info is-hidden icon is-small is-danger" data-balloon="Information required!" data-balloon-pos="up" data-balloon-length="medium">
		                    <i class="fal fa-exclamation-circle" data-fa-transform="up-3 left-4"></i>
		                  </div>
										</span>
	                </a>
	              </li>
	            </ul>
	          </div>
	        </div>

          <!-- ACCOUNT SETTINGS TAB -->
					<div id="account" class="columns is-laptop is-hidden drop-tab">

	          <!-- ACCOUNT INFORMATION (EMAIL, USERNAME, PW) -->
						<div class="column">
							<div class="card is-fullwidth">
								<div class="card-header">
									<h3 class="card-header-title">
	                  Account Information
	                </h3>
	              </div>
	              <div class="card-content">
	                <form id="account-details-form" class="control">
	                  <div class="control">
	                    <label class="label" for="email-input">
	                      Email Address
	                    </label>
	                    <input required id="email-input" class="input account-input" data-uservar="email" type="email" value="<%= user.email %>" oninvalid="this.setCustomValidity('Please enter a valid email address!')" oninput="setCustomValidity('')"/>
	                  </div>
	                  <div class="control">
	                    <label class="label" for="username-input">
	                      Username
	                      <div class="icon is-tooltip is-small" data-balloon="Must be between 3 and 50 characters and cannot include any spaces." data-balloon-pos="up" data-balloon-length="medium">
	                        <i class="fal fa-question-circle" data-fa-transform="down-3"></i>
	                      </div>
	                    </label>
	                    <input required id="username-input" minlength="3" maxlength="50" class="input account-input" data-uservar="username" type="text" value="<%= user.username %>" spellcheck="false" oninvalid="this.setCustomValidity('Please enter a valid username!')" oninput="setCustomValidity('')"/>
	                  </div>
  	                <div class="control is-spaced is-mobile">
  	                  <div class="control-left">
  	                    <p>
  	                      Password
  	                    </p>
  	                    <small class="tip">Click the button to change your password.</small>
  	                  </div>
  	                  <div class="control-right">
  	                    <a id="change-password-button" class="button is-stylish is-small">
  	                      <div class="icon is-small">
  													<i class="fal fa-unlock-alt"></i>
  												</div>
  	                      <span>Change Password</span>
  	                    </a>
  	                  </div>
  	                </div>
                    <div class="control">
                      <label class="label" for="ga_tracking_id-input">
                        Google Analytics Tracking ID
                      </label>
                      <small class="tip">Use a custom tracking ID to see additional traffic details on Google Analytics. <a class="is-primary is-bold" target="_blank" href="https://support.google.com/analytics/answer/1008080?hl=en#trackingID">Where can I find this?</a></small>
                      <input id="ga_tracking_id-input" class="input account-input" data-uservar="ga_tracking_id" type="text" value="<%= user.ga_tracking_id %>" spellcheck="false" oninvalid="this.setCustomValidity('Please enter a valid Google Analytics tracking ID!')" oninput="setCustomValidity('')"/>
                    </div>
                    <div class="control is-spaced is-mobile">
                      <div class="control-left">
                        <p>
                          Export Listings Data
                        </p>
                        <small class="tip">Download all of your DomaHub listings information as a CSV file.</a></small>
                      </div>
                      <div class="control-right">
                        <a id="csv-export-button" class="button is-stylish is-small">
                          <div class="icon is-small">
                            <i class="fal fa-download"></i>
                          </div>
                          <span>Download</span>
                        </a>
                      </div>
                    </div>
                  </form>
								</div>
							</div>
						</div>

	          <!-- REGISTRAR INFORMATION -->
	          <div class="column">
	            <div class="card is-fullwidth">
	              <div class="card-header">
	                <h3 class="card-header-title">
	                  My Registrars
	                </h3>
	              </div>
	              <div class="card-content">

	                <div class="control is-spaced is-mobile">
	                  <div class="control-left">
                      <figure class="image is-logo left-aligned">
                        <img src="/images/companies/godaddy-logo.png" alt="GoDaddy Logo" />
                      </figure>
	                    <small data-registrar="godaddy" data-registrar_name="GoDaddy" class="tip registrar-tip"></small>
	                  </div>
	                  <div class="control-right">
	                    <a data-registrar="godaddy" class="add-registrar-button button is-stylish is-small">
	                      <div class="icon is-small">
													<i class="fal fa-link"></i>
												</div>
	                      <span class="registrar-button-text" data-registrar_name="GoDaddy"></span>
	                    </a>
	                  </div>
	                </div>

	                <div class="control is-spaced is-mobile">
	                  <div class="control-left">
                      <figure class="image is-logo left-aligned">
                        <img src="/images/companies/namecheap-logo.png" alt="Namecheap Logo" />
                      </figure>
	                    <small data-registrar="namecheap" data-registrar_name="Namecheap" class="tip registrar-tip"></small>
	                  </div>
	                  <div class="control-right">
	                    <a data-registrar="namecheap" class="add-registrar-button button is-stylish is-small">
	                      <div class="icon is-small"><i class="fal fa-link"></i></div>
	                      <span class="registrar-button-text" data-registrar_name="NameCheap"></span>
	                    </a>
	                  </div>
	                </div>

	                <div class="control is-spaced is-mobile">
	                  <div class="control-left">
                      <figure class="image is-logo left-aligned">
                        <img src="/images/companies/namesilo-logo.png" alt="Namesilo Logo" />
                      </figure>
	                    <small data-registrar="namesilo" data-registrar_name="NameSilo" class="tip registrar-tip"></small>
	                  </div>
	                  <div class="control-right">
	                    <a data-registrar="namesilo" class="add-registrar-button button is-stylish is-small">
	                      <div class="icon is-small"><i class="fal fa-link"></i></div>
	                      <span class="registrar-button-text" data-registrar_name="NameSilo"></span>
	                    </a>
	                  </div>
	                </div>
	              </div>
	            </div>
	          </div>
	        </div>

          <!-- PREMIUM / REFERRAL / PROMO CODE TAB -->
	        <div id="premium" class="columns is-laptop is-hidden drop-tab">
	          <div class="column">
		          <div id="premium-upgrade-card" class="card is-fullwidth remove-margin-bottom-content">
	          		<div id="premium-card-header" class="card-header">
		          		<h3 id="upgrade-tab-header" class="card-header-title">
		          			<span>Premium Status</span>
		          		</h3>
		          	</div>
                <div class="basic-elem card-image">
  								<div class="notification no-shadow has-icon is-primary">
  									<div class="icon-box">
  										<div class="icon is-white">
  											<i class="fal fa-trophy-alt"></i>
  										</div>
  									</div>
  									<div class="content">
  										<small class="is-white">
  											Sign up for Premium and gain access to additional features like no commission fees, design customization, unlimited domains, etc.
  											<a tabindex="0" target="_blank" class="is-white is-bold" href="/features#pricing">
  												Click here to learn more.
  											</a>
  										</small>
  									</div>
  								</div>
  							</div>
                <div id="premium-expire-notification" class="is-hidden card-image">
                  <div class="notification no-shadow has-icon is-danger">
                    <div class="icon-box">
                      <div class="icon is-white">
                        <i class="fal fa-exclamation-triangle"></i>
                      </div>
                    </div>
                    <div class="content">
                      <small class="is-white">
                        Your Premium account is currently set to expire on <span class="exp-date-text"></span>. You will lose access to additional features like no commission fees, design customization, unlimited domains, etc.
                        <a tabindex="0" target="_blank" class="is-white is-bold" href="/features#pricing">
                          Click here to learn more.
                        </a>
                      </small>
                    </div>
                  </div>
                </div>
		          	<div class="card-content">
		          		<div class="control is-spaced">
		          			<div class="control-left">
		          				<p>
		          					Premium Status
		          				</p>
		          				<small id="renew-status" class="tip"></small>
		          			</div>
		          			<div class="control-right">
                      <a id="upgrade-button-annual" class="upgrade-button button is-stylish is-small is-primary is-outlined">
                        <div class="icon is-small">
                          <i class="fal fa-trophy"></i>
                        </div>
                        <span class="button-text">Yearly Plan - $50</span>
                      </a>
                      <a id="upgrade-button" class="upgrade-button button is-stylish is-small is-primary is-outlined">
                        <div class="icon is-small">
                          <i class="fal fa-trophy-alt"></i>
                        </div>
                        <span class="button-text">Monthly Plan - $5</span>
                      </a>
		          				<div class="control">
		          					<a id="cancel-premium-confirm-button" class="is-hidden is-outlined button is-danger is-stylish is-small">
		          						<div class="icon is-small">
		          							<i class="fal fa-ban"></i>
		          						</div>
		          						<span>Cancel Premium</span>
		          					</a>
		          					<a id="renew-premium-button" class="is-hidden is-outlined button is-primary is-stylish is-small">
		          						<div class="icon is-small">
		          							<i class="fal fa-repeat"></i>
		          						</div>
		          						<span>Renew Premium</span>
		          					</a>
		          				</div>
		          			</div>
		          		</div>
		          		<div class="control is-spaced is-mobile">
		          			<div class="control-left">
		          				<p>
		          					Default Payment Method
		          				</p>
		          				<small class="tip existing-cc">Click the button to add a default credit card!</small>
		          			</div>
		          			<div class="control-right">
                      <a id="delete-card-button" tabindex="0" title="Delete Payment Method" class="button is-hidden is-danger is-outlined is-stylish is-small">
                        <div class="icon is-small">
                          <i class="fal fa-trash-alt"></i>
                        </div>
                        <span>Remove Card</span>
                      </a>
		          				<a id="change-card-button" tabindex="0" title="Change Payment Method" class="button is-stylish is-small">
		          					<div class="icon is-small">
		          						<i class="fal fa-credit-card"></i>
		          					</div>
		          					<span>Add A New Card</span>
		          				</a>
		          			</div>
		          		</div>
                  <div id="unused-credits-section" class="control is-spaced">
                    <div class="control-left">
                      <p>
                        Total Unused Credits
                      </p>
                      <small id="unused-credit-status" class="tip"></small>
                    </div>
                    <div class="control-right">
                      <p class="unused-credits is-bold">
                        Loading...
                      </p>
                    </div>
                  </div>
		          	</div>
		          </div>

		          <div id="payments-history-card" class="premium-elem card is-fullwidth is-hidden">
		          	<div class="card-header is-block">
		          		<h3 class="card-header-title">
		          			<span>Premium Payments History</span>
		          		</h3>
		              <small id="next-charge-tip" class="tip is-inline-block"></small>
		          	</div>
		          	<div class="card-content">
		              <table id="payment-history-table" class="table">
		                <thead>
		                  <tr>
		                    <th>Date</th>
		                    <th>Amount</th>
		                  </tr>
		                </thead>
		                <tbody id="payment-history-body">
		                  <tr id="payment-history-clone" class="is-hidden payment-history-row no-hover-effect">
		                    <td class="payment-history-date"></td>
		                    <td class="payment-history-amount"></td>
		                  </tr>
		                </tbody>
		              </table>
		          	</div>
		          </div>
	          </div>
	          <div class="column">
	            <div class="card is-fullwidth">
	          	<div class="card-header">
	          		<h3 class="card-header-title">
	          			My Referrals and Promo Codes
	          		</h3>
	          	</div>
	            <div class="card-content">
	              <div id="promo-control" class="control is-spaced is-mobile">
	                <div class="control-left">
	                  <label class="label" for="promo-input">
	                    Promo Code
	                  </label>
	          				<small class="tip">Enter any promo codes here.</small>
	                </div>
	                <div class="control-right">
	                  <form id="promo-form">
	                    <div class="control has-icon has-addons">
	                      <input id="promo-input" autocomplete="off" required tabindex="0" class="input promo-input" type="text"  oninvalid="this.setCustomValidity('Please enter a valid promo code!')" oninput="setCustomValidity('')"/>
	                      <div class="icon">
													<i class="fal fa-tags"></i>
												</div>
	                      <button id="promo-submit" class="promo-input button is-stylish is-small no-shadow" text="Submit" type="submit" form="promo-form">
													<div class="icon is-small">
														<i class="fal fa-check-circle"></i>
													</div>
													<span>Submit</span>
												</button>
	                    </div>
	                  </form>
	                </div>
	              </div>
	          		<div class="control">
	          			<label class="label" for="referral-link">
	                  My Referrals
	                  <div class="icon is-tooltip is-small" data-balloon="Once the referred account has signed up for a Premium account, you will both be credited with a free month of Premium!" data-balloon-pos="up" data-balloon-length="large">
	                    <i class="fal fa-question-circle" data-fa-transform="down-3"></i>
	                  </div>
	                </label>
	          			<small class="tip">
	          				These are people who have signed up using your referral link or any promotional codes you may have applied to your account.
	          			</small>
	                <div class="control has-addons">
	                  <input id="referral-link" value="https://domahub.com/signup/<%= user.username.toLowerCase() %>" class="input" placeholder="https://domahub.com/signup/<%= user.username.toLowerCase() %>" />
	                  <a id="referral-link-copy" class="button is-stylish is-small no-shadow" tabindex="0">
	                    <div class="icon is-small">
												<i class="fal fa-clipboard"></i>
											</div>
	                    <span id="referral-link-text">Copy Link</span>
	                  </a>
	                </div>
	          		</div>
	              <table id="loading-referral-table" class="table">
	          			<tbody>
	                  <tr class="no-hover-effect">
	                    <td>
	                      <div class="nothing-exists-row has-text-centered">
	                        <p class='icon is-medium is-primary'><i class="fal fa-spin fa-circle-notch"></i></p>
	                        <p>Now loading your referrals.</p>
	                        <p>This may take some time.</p>
	                      </div>
	                    </td>
	                  </tr>
	          			</tbody>
	              </table>
	              <table id="no-referral-table" class="table is-hidden">
	          			<tbody>
	                  <tr id="no-referrals" class="no-hover-effect">
	                    <td colspan="3">
	                      <div class="nothing-exists-row content has-text-centered">
	                        <figure class="image is-64x64">
	                          <img src="/images/lib/smile.png" />
	                        </figure>
	                        <p>
	                          You have no referrals yet. Let's invite some friends!
	                        </p>
	                      </div>
	                    </td>
	                  </tr>
	          			</tbody>
	              </table>
	              <table id="referral-table" class="table is-hidden">
                  <thead>
                    <tr>
                      <th colspan="3">
                        Total earned: <span id="total-amount-redeemed"></span>
                      </th>
                      <th>
                        <p>
                          <a id="refresh-referral-table-button" class="is-primary is-bold text-normal is-pulled-right">
                            Click here to refresh
                          </a>
                        </p>
                      </th>
                    </tr>
                    <tr>
                      <th>Type</th>
                      <th class="is-hidden-456px">
                        Status
                        <div class="icon is-tooltip is-small" data-balloon="The current status of the referred account or a promotional code." data-balloon-pos="up" data-balloon-length="medium">
                          <i class="fal fa-question-circle"></i>
                        </div>
                      </th>
                      <th class="has-text-right">Amount</th>
                      <th class="has-text-right">Date Applied</th>
                    </tr>
                  </thead>
	          			<tbody>
	          				<tr id="referral-clone" class="is-hidden referral-row no-hover-effect">
	          					<td class="referral-type"></td>
                      <td class="referral-redeemed is-hidden-456px"></td>
	          					<td class="referral-months has-text-right"></td>
	                    <td class="referral-created has-text-right"></td>
	          				</tr>
	          			</tbody>
	              </table>
	          	</div>
	          </div>
	          </div>
	        </div>

	        <!-- PAYMENTS INFORMATION TAB -->
	        <div id="payment" class="columns is-laptop is-hidden drop-tab">
	          <div class="column">
	            <form id="stripe-personal-form" autocomplete="on">

	              <!-- PERSONAL INFO -->
	              <div class="card is-fullwidth">
	                <div class="card-header is-block">
	                  <h3 class="card-header-title">
											<span>Personal Information</span>
											<span class="stripe-required-info tag is-small is-danger">Incomplete</span>
										</h3>
	                  <small class="tip">
	                    This information is legally required for payments processing.
											DomaHub does not share this information with anyone except our payments processors unless legally required.
                    </small>
	                </div>
	                <div class="card-content">
	                  <div class="control">
	                    <label class="label" for="first_name-input">First Name</label>
	                    <input required data-uservar="first_name" autocomplete="additional-name" id="first_name-input" class="input stripe-account-input" type="text" name="first_name" oninvalid="this.setCustomValidity('Please enter a valid first name!')" oninput="setCustomValidity('')"/>
	                  </div>
	                  <div class="control">
	                    <label class="label" for="last_name-input">Last Name</label>
	                    <input required data-uservar="last_name" autocomplete="given-name" id="last_name-input" class="input stripe-account-input" type="text" name="last_name" oninvalid="this.setCustomValidity('Please enter a valid last name!')" oninput="setCustomValidity('')"/>
	                  </div>
	                  <div class="control">
	                    <label class="label" for="last_name-input">Phone Number</label>
	                    <input required data-uservar="phone_number" autocomplete="phone-number" id="phone_number-input" class="input stripe-account-input" type="text" name="phone_number" oninvalid="this.setCustomValidity('Please enter a valid phone number!')" oninput="setCustomValidity('')"/>
	                  </div>
	                  <div class="control">
	                    <label class="label">Birth Date</label>
	                    <div class="select">
	                      <select required data-uservar="birthday_year" autocomplete="bday-year" id="birthday_year-input" name="birthday_year" class="stripe-account-input" oninvalid="this.setCustomValidity('Please select a valid birth year!')" oninput="setCustomValidity('')">
	                        <option selected value="" disabled>Year</option>
	                        <option value="1998">1998</option>
	                        <option value="1997">1997</option>
	                        <option value="1996">1996</option>
	                        <option value="1995">1995</option>
	                        <option value="1994">1994</option>
	                        <option value="1993">1993</option>
	                        <option value="1992">1992</option>
	                        <option value="1991">1991</option>
	                        <option value="1990">1990</option>
	                        <option value="1989">1989</option>
	                        <option value="1988">1988</option>
	                        <option value="1987">1987</option>
	                        <option value="1986">1986</option>
	                        <option value="1985">1985</option>
	                        <option value="1984">1984</option>
	                        <option value="1983">1983</option>
	                        <option value="1982">1982</option>
	                        <option value="1981">1981</option>
	                        <option value="1980">1980</option>
	                        <option value="1979">1979</option>
	                        <option value="1978">1978</option>
	                        <option value="1977">1977</option>
	                        <option value="1976">1976</option>
	                        <option value="1975">1975</option>
	                        <option value="1974">1974</option>
	                        <option value="1973">1973</option>
	                        <option value="1972">1972</option>
	                        <option value="1971">1971</option>
	                        <option value="1970">1970</option>
	                        <option value="1969">1969</option>
	                        <option value="1968">1968</option>
	                        <option value="1967">1967</option>
	                        <option value="1966">1966</option>
	                        <option value="1965">1965</option>
	                        <option value="1964">1964</option>
	                        <option value="1963">1963</option>
	                        <option value="1962">1962</option>
	                        <option value="1961">1961</option>
