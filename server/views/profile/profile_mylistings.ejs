<!DOCTYPE html>

<html class="prevent-scroll-jump">

<head>
	<title>My Listings - DomaHub</title>
	<link rel="stylesheet" href="/css/min/profile_mylistings.min.css" />
	<link rel="stylesheet" href="/css/lib/jquery.tagit.css" type="text/css" />
	<link rel="stylesheet" href="/css/lib/jquery.minicolors.css" type="text/css" />
	<%- include("../templates/head_links"); %>
</head>

<body>

	<%- include("profile_nav.ejs"); %>

	<section class="section is-fullscreen">
		<div id="listing-msg-error" class="is-hidden is-danger notification is-popup">
			<button class="delete"></button>
      <p id="listing-msg-error-text" class='is-white'></p>
      <a id="error-upgrade-button" href="/profile/settings#premium" class="margin-top-15 is-hidden button is-stylish is-danger is-inverted is-small">
        <span class="icon is-small">
          <i class="fa fa-diamond"></i>
        </span>
        <span>Click Here To Upgrade</span>
      </a>
		</div>
		<div id="listing-msg-success" class="is-hidden is-primary notification is-popup">
			<button class="delete"></button>
			<p id="listing-msg-success-text" class='is-white'></p>
		</div>
		<div class="wrapper">

			<%- include("profile_left_menu.ejs"); %>

			<!-- LISTING DETAILS  -->
			<div class="right-content">
				<div class="columns">
					<!-- DOMAINS TABLE -->
					<div class="column is-narrow">
						<div id="filter-menu" class="">
						  <!-- FITER MENU -->
							<div class="content">
								<h2>My Listings</h2>
							</div>
							<div class="button-wrapper control margin-top-10">
						    <button id="multi-delete" class="button is-disabled is-danger is-transparent" tabindex="0" title="Delete Listing(s)">
						      <span class="icon is-small">
						        <i class="fa fa-trash"></i>
						      </span>
						      <span id="multi-delete-text">Delete</span>
						    </button>
						    <button id="multi-verify" class="button is-disabled is-primary is-transparent" tabindex="0" title="Verify Listing(s)">
						      <span class="icon is-small">
						        <i class="fa fa-check-circle-o"></i>
						      </span>
						      <span>Verify</span>
						    </button>
								<button id="multi-edit" class="button is-disabled is-info is-transparent" tabindex="0" title="Verify Listing(s)">
						      <span class="icon is-small">
						        <i class="fa fa-cogs"></i>
						      </span>
						      <span>Edit</span>
						    </button>
								<a class="button is-black is-inverted" tabindex="0" href="/listings/create">
						      <span class="icon is-small">
						        <i class="fa fa-plus"></i>
						      </span>
						      <span>Create Listings</span>
						    </a>
						  </div>
						  <div id="filters" class="margin-bottom-20">
						    <div class="control is-grouped">
									<div class="control">
										<button class="button is-black is-inverted no-shadow">
											View All
										</button>
									</div>
									<div class="control">
										<span class="select no-shadow no-borders">
											<select>
												<option>Verified</option>
												<option>Unverified</option>
											</select>
										</span>
									</div>
									<div class="control">
										<span class="select no-shadow no-borders">
											<select>
												<option>Active</option>
												<option>Inactive</option>
											</select>
										</span>
									</div>
									<div class="control">
										<span class="select no-shadow no-borders">
											<select>
												<option>Sold (Not Transferred)</option>
												<option>Sold (Transferred)</option>
											</select>
										</span>
									</div>
									<div class="control has-icon">
										<i class="fa fa-search"></i>
										<input id="domain-search" tabindex="0" class="input is-expanded" type="text" placeholder="Search" />
									</div>
						    </div>
						  </div>
							<!-- DOMAINS TABLE -->
							<table class="table is-bordered">
								<thead class="has-hover-effect">
									<tr>
										<th>
											<form id="select-all" class="checkbox-form">
								        <input class="select-button" data-selected="false" type="checkbox" tabindex="0" title="Select All" />
								        <label for="select-button"></label>
								      </form>
										</th>
										<th>Domain Name</th>
										<th>Created</th>
										<th>Min. Offer</th>
										<th>BIN</th>
										<th>Theme</th>
									</tr>
								</thead>
								<tbody id="table-body">
									<tr id="no-listings-row" class="padding-15 clone-row has-text-centered is-hidden">
							     	<td colspan="6">
											<span class="icon is-small"><i class="fa fa-frown-o"></i></span>
										 	<span>There are no matching listings!</span>
										</td>
							    </tr>
									<tr id="verified-clone-row" class="clone-row table-row is-hidden">
										<td>
											<form class="checkbox-form">
								        <input class="select-button" data-selected="false" type="checkbox" tabindex="0" title="Select" />
								        <label for="select-button"></label>
								      </form>
										</td>
										<td class="td-domain"></td>
										<td class="td-date"></td>
										<td class="td-min"></td>
										<td class="td-bin"></td>
										<td class="td-theme"></td>
									</tr>
									<tr id="unverified-clone-row" class="clone-row table-row is-unverified is-hidden">
										<td>
											<form class="checkbox-form">
								        <input class="select-button" data-selected="false" type="checkbox" tabindex="0" title="Select" />
								        <label for="select-button"></label>
								      </form>
										</td>
										<td class="td-domain">
											<span class="icon is-danger is-small is-inline">
							          <i class="fa fa-exclamation-circle"></i>
							        </span>
										</td>
										<td class="td-date"></td>
										<td class="td-min"></td>
										<td class="td-bin"></td>
										<td class="td-theme"></td>
									</tr>
								</tbody>
							</table>
							<!-- <div id="table-body" class="margin-top-15">
						    <div id="verified-clone-row" class="clone-row table-row is-hidden">
						      <form class="checkbox-form">
						        <input class="select-button" data-selected="false" type="checkbox" tabindex="0" title="Select" />
						        <label for="select-button"></label>
						      </form>
						      <p class='td-domain'></p>
						    </div>
						    <div id="unverified-clone-row" class="clone-row table-row is-hidden">
						      <form class="checkbox-form">
						        <input class="select-button" data-selected="false" type="checkbox" tabindex="0" title="Select" />
						        <label for="select-button"></label>
						      </form>
						      <div class="table-row-text">
						        <p class="td-domain is-danger is-inline"></p>
						        <span class="icon is-danger is-small is-inline">
						          <i class="fa fa-exclamation-circle"></i>
						        </span>
						      </div>
						    </div>
						  </div> -->
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<script src="/js/jquery/jquery-ui.js" type="text/javascript"></script>
	<script src="/js/lib/moment.min.js" type="text/javascript"></script>
	<script src="/js/lib/tag-it.min.js" type="text/javascript"></script>
	<script src="/js/lib/wNumb.js" type="text/javascript"></script>
	<script src="/js/jquery/jquery.minicolors.min.js" type="text/javascript"></script>
  <script src="/js/themes.js" type="text/javascript"></script>
  <script src="/js/lib/chart.min.js" type="text/javascript"></script>
  <script src="/js/lib/randomColor.js" type="text/javascript"></script>
	<script type="text/javascript">
		var user = <% if(typeof user != "undefined"){ %><%-JSON.stringify(user)%><% }else{ %>window.location="/"<% } %>;
		var listings = <% if(typeof listings != "undefined"){ %><%-JSON.stringify(listings)%><% }else{ %>""<% } %>;
		var categories = <% if(typeof categories != "undefined"){ %><%-JSON.stringify(categories)%><% }else{ %>""<% } %>;
		var fonts = <% if(typeof fonts != "undefined"){ %><%-JSON.stringify(fonts)%><% }else{ %>""<% } %>;
	</script>
	<script src="/js/routes/profile/profile_mylistings_logic.js" type="text/javascript"></script>
</body>

</html>
