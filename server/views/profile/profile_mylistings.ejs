                        <a href="/profile/settings#premium" class="button is-stylish no-shadow is-primary is-small">
                          <span class="icon is-small">
                            <i class="fa fa-diamond"></i>
                          </span>
                          <span>Upgrade</span>
                        </a>
                      </div>
										</div>
