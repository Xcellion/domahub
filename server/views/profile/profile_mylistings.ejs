<!DOCTYPE html>

<html>

<head>
	<title>My Listings - DomaHub</title>
	<link rel="stylesheet" href="/css/min/profile_mylistings.min.css" />
	<link rel="stylesheet" href="/css/lib/jquery.tagit.css" type="text/css" />
	<link rel="stylesheet" href="/css/lib/jquery.minicolors.css" type="text/css" />
	<%- include("../templates/head_links"); %>
</head>

<body>

	<%- include("profile_nav.ejs"); %>

	<section class="section is-fullscreen">
		<div id="listing-msg-error" class="is-hidden is-danger notification is-popup">
			<button class="delete"></button>
      <p id="listing-msg-error-text" class='is-white'></p>
		</div>
		<div id="listing-msg-success" class="is-hidden is-primary notification is-popup">
			<button class="delete"></button>
			<p id="listing-msg-success-text" class='is-white'></p>
		</div>
		<div class="wrapper">

			<%- include("profile_left_menu.ejs"); %>

			<div class="right-content">

        <!-- DOMAINS SELECTOR -->
        <%- include("profile_domain_selector.ejs"); %>

        <!-- DOMAINS EDITOR -->
        <%- include("profile_domain_editor.ejs"); %>

			</div>
		</div>
	</section>

	<script src="/js/jquery/jquery-ui.js" type="text/javascript"></script>
	<script src="/js/lib/moment.min.js" type="text/javascript"></script>
	<script src="/js/lib/tag-it.min.js" type="text/javascript"></script>
	<script src="/js/lib/wNumb.js" type="text/javascript"></script>
	<script src="/js/jquery/jquery.minicolors.min.js" type="text/javascript"></script>
  <script src="/js/themes.js" type="text/javascript"></script>
  <script src="/js/lib/chart.min.js" type="text/javascript"></script>
  <script src="/js/lib/randomColor.js" type="text/javascript"></script>
	<script type="text/javascript">
		var user = <% if(typeof user != "undefined"){ %><%-JSON.stringify(user)%><% }else{ %>window.location="/"<% } %>;
		var listings = <% if(typeof listings != "undefined"){ %><%-JSON.stringify(listings)%><% }else{ %>""<% } %>;
		var categories = <% if(typeof categories != "undefined"){ %><%-JSON.stringify(categories)%><% }else{ %>""<% } %>;
		var fonts = <% if(typeof fonts != "undefined"){ %><%-JSON.stringify(fonts)%><% }else{ %>""<% } %>;
	</script>
  <script src="/js/routes/profile/profile_listing_selector_logic.js" type="text/javascript"></script>
	<script src="/js/routes/profile/profile_listing_editor_logic.js" type="text/javascript"></script>
</body>

</html>
