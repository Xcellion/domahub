<div id="domain-editor" class="columns is-hidden">

  <!-- LISTING EDIT TOOLBAR -->
  <div id="edit-toolbar" class="edit-elem non-offer-elem non-verify-elem tabs-menu column">

    <!-- CURRENT DOMAIN NAME -->
    <div class="content">
      <a class="show-selector-button button is-black is-transparent no-shadow" tabindex="0" title="Back To My Listings">
        <span class="icon is-small">
          <i class="fa fa-long-arrow-left"></i>
        </span>
        <span>Go Back To Listings</span>
      </a>
    </div>
    <div class="content">
      <h2 class="title-wrapper is-inline-block">
        <span class="editor-title"></span>
        <span class="current-domain-name"></span>
      </h2>
    </div>

    <div class="menu">
      <ul class="menu-list">
        <li class="menu-item has-icon">
          <a id="info-tab" tabindex="0" class="unpurchased-elem tab primary-theme is-active">
            <span class="icon is-small"><i class="fa fa-info"></i></span>
            <p>Information</p>
          </a>
        </li>
        <li class="menu-item has-icon">
          <a id="design-tab" tabindex="0" class="unpurchased-elem tab">
            <span class="icon is-small"><i class="fa fa-magic"></i></span>
            <p>Design</p>
          </a>
        </li>
        <li class="menu-item has-icon">
          <a id="rental-tab" tabindex="0" class="unpurchased-elem tab">
            <span class="icon is-small"><i class="fa fa-calendar-check-o"></i></span>
            <p>Rental</p>
          </a>
        </li>
        <hr class="is-hidden-mobile"/>
        <li class="menu-item has-icon">
          <a id="status-toggle-button" tabindex="0" class="button no-shadow verify-elem" title="Publicly hide or show landing page(s).">
            <span id="status-color" class="icon is-small"><i id="status-icon" class="fa"></i></span>
            <span id="status-text"></span>
          </a>
        </li>
        <li class="menu-item has-icon">
          <div id="view-listings-button-wrapper" class="is-relative">
            <a id="view-listings-button" target="_blank" rel="noopener" tabindex="0">
              <span class="icon is-small"><i class="fa fa-external-link"></i></span>
              <span>View Listing<span class="edit-domain-plural is-hidden">s</span></span>
            </a>
            <div id="view-listings-button-drop" class="box domain-tooltip is-hidden"></div>
          </div>
        </li>
      </ul>
    </div>
  </div>

  <div class="column">

    <!-- OFFERS TABLE -->
    <div id="offers-section" class="offer-elem non-edit-elem non-verify-elem is-hidden">
      <div class="content">
        <a class="show-selector-button button is-black is-transparent no-shadow" tabindex="0" title="Back To Domain Selector">
          <span class="icon is-small">
            <i class="fa fa-long-arrow-left"></i>
          </span>
          <span>Go Back To Listings</span>
        </a>
      </div>

      <div id="offers-toolbar" class="control">
        <div class="content">
          <h2 class="title-wrapper is-bold">
            <span class="editor-title">My Offers</span>
            <span class="current-domain-name"></span>
          </h2>
        </div>
        <div class="offer-button control button-wrapper">
          <a id="refresh-offers-button" class="is-hidden button is-black is-transparent" title="Refresh Offers">
            <span class="icon is-small"><i class="fa fa-refresh"></i></span>
            <span>Refresh Offers</span>
          </a>
          <a id="show-rejected-offers" class="button is-black is-transparent">
            <span class="icon"><i class="fa fa-toggle-off"></i></span>
            <span class="button-text">Show Rejected</span>
          </a>
        </div>
        <div class="offer-button control is-grouped">
          <div class="control has-icon">
            <span class="select">
              <select id="offers-sort-select" tabindex="0">
                <option selected value="timestamp_desc">Date Received (Newest)</option>
                <option value="timestamp_asc">Date Received (Oldest)</option>
                <option value="offer_desc">Price (High - Low)</option>
                <option value="offer_asc">Price (Low - High)</option>
                <option value="name_asc">Offerer Name (A - Z)</option>
                <option value="name_desc">Offerer Name (Z - A)</option>
                <option value="accepted_desc">Rejected / Accepted</option>
                <option value="accepted_asc">Accepted / Rejected</option>
              </select>
            </span>
            <i class="fa fa-sort"></i>
          </div>
          <div class="control has-icon">
            <i class="fa fa-search"></i>
            <input id="offer-search" tabindex="0" class="input is-expanded" type="text" placeholder="Search Offers" />
          </div>
        </div>
      </div>
      <div>
        <div id="offer-modal" class="modal">
          <div class="modal-background"></div>
          <div class="modal-container">
            <div class="modal-content offer-popup">
              <div class="content has-text-centered">
                <h3 class="is-bold" id="offer-modal-domain"></h3>
                <p>Received on <span id="offer-modal-timestamp"></span></p>
              </div>
              <div class="content">
                <h1 id="offer-modal-price" class="has-text-centered"></h1>
              </div>
              <div class="content">
                <ul id="offer-info" class="offer-info">
                  <li>
                    <span class="icon is-small" title="From"><i class="fa fa-user-o"></i></span><span id="offer-modal-name"></span>
                  </li>
                  <li>
                    <span class="icon is-small" title="Email"><i class="fa fa-envelope-o"></i></span><span id="offer-modal-email"></span>
                  </li>
                  <li>
                    <span class="icon is-small" title="Phone"><i class="fa fa-phone"></i></span><span id="offer-modal-phone"></span>
                  </li>
                  <li class="">
                    <span class="icon is-small" title="Message"><i class="fa fa-comment-o"></i></span>
                    <p id="offer-modal-message" class="offer-msg"></p>
                  </li>
                </ul>
                <ul id="offer-response-label" class="is-hidden">
                  Your response:
                </ul>
                <ul id="offer-response-wrapper">
                  <li>
                    <textarea id="offer-response" placeholder="You may include an optional response to the offerer." class='textarea'></textarea>
                  </li>
                </ul>
              </div>
              <div id="offer-modal-button-wrapper" class="control is-grouped is-grouped-center">
                <p class="control">
                  <a id="accept_button" tabindex="0" class='button is-primary '>
                    <span class="icon is-small"><i class="fa fa-check"></i></span>
                    <span>Accept offer</span>
                  </a>
                </p>
                <p class="control">
                  <a id="reject_button" tabindex="0" class='button is-outlined is-danger '>
                    <span class="icon is-small"><i class="fa fa-times"></i></span>
                    <span>Reject offer</span>
                  </a>
                </p>
              </div>
            </div>
          </div>
          <button class="modal-close"></button>
        </div>
        <div id="deposited-offer" class="hidden-while-loading-offers has-border-bottom is-hidden">
          <div class="content">
            <h3>
              Congratulations on selling your domain!
            </h3>
            <p>
              The money has been deposited and verified by DomaHub. You must now transfer the ownership of this domain to the new owner.
              Once the new owner has verified that the ownership has been transferred over, the money will become available for withdrawal.
              If you do not know how to manually transfer a domain, please refer to our <a target="_blank" href="https://medium.com/@domahub/how-to-transfer-your-domain-9d3f93070292" class="is-primary is-underlined">Domain Transfer Guide</a>.
            </p>
            <p class="is-bold">
              This domain must be transferred by <span id="deposited-deadline"></span>.
            </p>
            <p class="is-bold">
              Failure to transfer ownership of the domain within the next 7 days will result in the money being refunded. Your account will be also be flagged for review.
            </p>
            <p>
              <p class="resend-wrapper">
                <a id="resend-deposit" class="button ">Re-send verification email to buyer</a>
              </p>
              <a class="is-primary" href="/contact">
                Having issues? Contact us for assistance.
              </a>
            </p>
          </div>
        </div>
        <div id="accepted-offer" class="hidden-while-loading-offers has-border-bottom is-hidden">
          <div class="content">
            <h3>
              I accepted the offer--what's next?
            </h3>
            <p>
              The buyer has been sent an email with instructions on completing payment.
              Once the payment has been verified, you will need to manually transfer the ownership of this domain to him/her.
              If you do not know how to manually transfer a domain, please refer to our <a target="_blank" href="/faq#transferguide" class="is-primary is-underlined">Domain Transfer Guide</a>.
            </p>
            <p>
              <span class="is-bold">There is no required action on your part just yet.</span> Hold tight, we are getting you your money.
            </p>
            <p>
              If you have changed your mind about this offer, please <a href="/contact" class="is-primary">let us know</a>.
            </p>
            <p class="resend-wrapper">
              <a id="resend-accept" class="button ">Re-send payment email to buyer</a>
            </p>
          </div>
        </div>
        <table class="table is-bordered">
          <thead>
            <tr>
              <th>Domain Name</th>
              <th>Offer</th>
              <th>Name</th>
              <th>Date</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody id="offers-wrapper">
            <tr id="loading-offers" class="no-hover-effect">
              <td colspan="5">
                <div class="nothing-exists-row has-text-centered">
                  <p class="icon is-medium"><i class="fa fa-spin fa-refresh"></i></p>
                  <p>Now loading domain offers...</p>
                </div>
              </td>
            </tr>
            <tr id="no-offers" class="is-hidden no-hover-effect">
              <td colspan="5">
                <div class="nothing-exists-row has-text-centered">
                  <img src="/images/lib/ggface.png" />
                  <p>Oh, no! There are no existing/matching offers!</p>
                </div>
              </td>
            </tr>
            <tr id="offer-clone" class="offer-row has-hover-effect is-hidden">
              <td class="td-offer-domain"></td>
              <td class="td-offer-offer"></td>
              <td class="td-offer-name"></td>
              <td class="td-offer-timestamp"></td>
              <td class="td-offer-status"></td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>

    <!-- VERIFICATION TABLE -->
    <div id="verify-section" class="verify-elem non-offer-elem non-edit-elem is-hidden">

      <div class="content">
        <a class="show-selector-button button is-black is-transparent no-shadow" tabindex="0" title="Back To Domain Selector">
          <span class="icon is-small">
            <i class="fa fa-long-arrow-left"></i>
          </span>
          <span>Go Back To Listings</span>
        </a>
      </div>
      <div class="content">
        <h2 class="title-wrapper is-bold">
          <span class="editor-title"></span>
          <span class="current-domain-name"></span>
        </h2>
      </div>
      <div class="content">
        <p>The table below details the DNS entries that are needed to verify <span class="verification-domains-plural"></span>.</p>
        <p>If you require any assistance, please refer to our <a tabindex="0" target="_blank" class="is-underlined is-primary" href="https://medium.com/@domahub/how-to-verify-domains-on-domahub-7d526c4ce6d"> step-by-step guide</a> on domain verification.</p>
        <p id="registrar_url">Please <a>log in to your respective domain provider<span class="verification-plural"></span></a> to create these entries.</p>
      </div>
      <table id="required-dns-table" class="table is-narrow">
          <thead>
            <tr>
              <th>Host</th>
              <th>Record</th>
              <th>Required Data</th>
            </tr>
          </thead>
          <tbody>
            <tr class='no-hover-effect'>
              <td class="dns_host">@</td>
              <td>A</td>
              <td class='required_data'>208.68.37.82</td>
            </tr>
            <tr class='no-hover-effect'>
              <td class="dns_host">www</td>
              <td>A</td>
              <td class='required_data'>208.68.37.82</td>
            </tr>
          </tbody>
        </table>
      <div class="content">
        <p id="verification-left" class="is-hidden is-danger">You have <span id="verification-left-num"></span> domain DNS settings left to modify.</p>
      </div>
      <div id="verify-toolbar" class="control">
        <div class="control is-grouped">
          <div class="control">
            <a id="current-dns-button" class="button is-hidden" title="Show Current DNS Settings">
              <span class="icon is-small"><i class="fa fa-eye"></i></span>
              <span>Show Current DNS Settings</span>
            </a>
            <a id="refresh-dns-button" class="button is-hidden" title="Refresh DNS Table">
              <span class="icon is-small"><i class="fa fa-refresh"></i></span>
              <span>Refresh DNS Table</span>
            </a>
            <a id="verify-button" class="is-primary button is-hidden" title="Verify domain(s)">
              <span class="icon is-small"><i class="fa fa-check-circle-o"></i></span>
              <span>Verify Now</span>
            </a>
          </div>
        </div>
      </div>
      <table id="current-dns-table" class="is-hidden table">
        <thead>
          <tr>
            <th>Domain Name</th>
            <th>Registrar</th>
            <th>Host</th>
            <th>Record</th>
            <th>Required Data</th>
            <th>Current Data</th>
            <th>Next Steps</th>
          </tr>
        </thead>
        <tbody id="dns_table-body">
          <tr id="loading-records-row" class="no-hover-effect">
            <td colspan=7 class="has-text-centered">
              <div class="nothing-exists-row has-text-centered">
                <p class="icon is-medium"><i class="fa fa-spin fa-refresh"></i></p>
                <p>Now loading existing DNS records...</p>
              </div>
            </td>
          </tr>
          <tr id="doma-a-record-clone" class='is-hidden no-hover-effect'>
            <td rowspan=2 class="verify_table_domain_name"></td>
            <td rowspan=2 class="verify_table_registrar">-</td>
            <td class="dns_host">@</td>
            <td>A</td>
            <td class='required_data'>208.68.37.82</td>
            <td class="existing_data"></td>
            <td class="next_step"><span class='is-danger'></span></td>
          </tr>
          <tr id="doma-www-record-clone" class='is-hidden no-hover-effect'>
            <td class="dns_host">www</td>
            <td>A</td>
            <td class='required_data'>208.68.37.82</td>
            <td class="existing_data"></td>
            <td class="next_step"><span class='is-danger'></span></td>
          </tr>
          <tr id="existing-dns-row" class='is-hidden no-hover-effect'>
            <td class="dns_host">@</td>
            <td>A</td>
            <td class='required_data'></td>
            <td class="existing_data"></td>
            <td class="next_step"><span class='is-danger'>Create this record</span></td>
          </tr>
        </tbody>
      </table>
    </div>

    <!-- LISTING DETAILS -->
    <div id="editing-section" class="edit-elem non-offer-elem non-verify-elem card is-fullwidth">
      <div class="card-header is-justified-start">
        <h3 id="tab-title" class="card-header-title"></h3>
        <div class="">
          <a id="save-changes-button" class="button is-primary is-hidden">
            <span class="icon is-small"><i class="fa fa-check-circle-o"></i></span>
            <span>Save</span>
          </a>
          <a id="cancel-changes-button" class="button is-danger is-inverted is-hidden">
            <span class="icon is-small"><i class="fa fa-ban"></i></span>
            <span>Cancel</span>
          </a>
        </div>
      </div>
			<div id="info-tab-drop" data-title="Edit Information Details" class="tab-drop card-content is-hidden">
				<div class="control">
					<label class="label" for="min-price-input">
            Minimum Price
          </label>
          <small class="tip">The minimum offer price you will consider. Leave blank or 0 to disable minimum offer price.</small>
					<input id="min-price-input" class="input changeable-input" data-name="min_price" type="number" min="1" step="1">
				</div>
        <div class="control">
          <label class="label" for="buy-price-input">
            Buy It Now Price
          </label>
          <small class="tip">The price you are willing to immediately sell the domain for. Leave blank or 0 to disable Buy It Now option.</small>
          <input id="buy-price-input" class="input changeable-input" data-name="buy_price" type="number" min="1" step="1">
        </div>
				<div class="control">
					<label class="label" for="description">
            Listing Description
          </label>
          <small class="tip">Say anything you want about your domain. This will appear as a paragraph below the domain name.</small>
					<textarea id="description" class="description-input textarea changeable-input"
					tabindex="0" data-name="description" placeholder="Write anything you want!"></textarea>
				</div>
        <div class="control">
          <label class="label" for="domain-name-input">
            Domain Capitalization
            <span class="icon is-small is-tooltip" data-balloon-length="large" data-balloon="Change the capitalization of your domain name to fit your custom domain branding." data-balloon-pos="up">
              <i class="fa fa-question-circle"></i>
            </span>
            <% if (!user.stripe_subscription_id) { %>
            <span class="icon is-small is-tooltip" data-balloon="Premium Feature" data-balloon-pos="up">
              <i class="fa fa-diamond"></i>
            </span>
            <% } %>
          </label>
          <small id="domain-name-cap-missing" class="tip is-danger">You cannot edit the domain name capitalization with more than one domain selected. Please select a specific domain to edit this setting.</small>
          <input id="domain-name-input" class="input premium-input changeable-input is-narrow" data-name="domain_name" type="text">
        </div>
        <div class="control">
          <label class="label" for="description-footer">
            <span>Footer Description</span>
            <% if (!user.stripe_subscription_id) { %>
            <span class="icon is-small is-tooltip" data-balloon="Premium Feature" data-balloon-pos="up">
              <i class="fa fa-diamond"></i>
            </span>
            <% } %>
          </label>
          <small class="tip">Insert up to 75 characters to be displayed in the footer of your sales page. An ideal location for a slogan, phrase, or trademark.</small>
          <input id="description-footer" data-name="description_footer" tabindex="0" class="premium-input input changeable-input" placeholder="The best domain names in the industry." />
        </div>
				<%# <div class="control">
					<label class="label" for="description-hook">Short Description</label>
					<input id="description-hook" data-name="description_hook" tabindex="0" class="input changeable-input" placeholder="What is this website?" />
					<small class="tip">100 character limit.</small>
				</div>%>
				<div class="control">
					<label class="label">
						<span>Categories</span>
						<span class="icon is-small is-tooltip" data-balloon-length="large" data-balloon="Categories help users find similar domains. Incorrect categories can lead to a poor user experience and hurt your domain brand by being associated with non-similar domains." data-balloon-pos="up">
							<i class="fa fa-question-circle"></i>
						</span>
					</label>
          <small class="tip">Don't see a category that fits your domain? <a class="is-primary" href="/contact">Let us know!</a></small>
					<div id="categories" class="category-control">
						<input id="categories-input" data-name="categories" onchange="" tabindex="0" class="is-hidden input changeable-input" value="" business="" ecard="" marketing="" niche="" personal="" promotion="">

						<%# make all the categories %>
