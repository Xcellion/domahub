<div id="domain-editor" class="is-hidden">

  <!-- EDIT LISTING DETAILS -->
  <div id="editing-section" class="edit-elem non-offer-elem non-stats-elem non-verify-elem">
    <!-- LISTING EDIT TOOLBAR -->
    <div id="edit-toolbar">

      <!-- EDITING BUTTONS -->
      <div class="content">
        <div class="control is-spaced">
          <div class="control-left">
            <a class="show-selector-button button is-flexible is-black is-transparent no-shadow" tabindex="0" title="Back To My Listings">
              <div class="icon is-small">
                <i class="fal fa-long-arrow-left"></i>
              </div>
              <span class="is-hidden-mobile">Go Back To Listings</span>
            </a>
          </div>
          <div class="control-right">
            <a id="save-changes-button" class="button is-flexible is-primary is-stylish is-small is-hidden">
              <div class="icon is-small"><i class="fal fa-check-circle"></i></div>
              <span>Save</span>
            </a>
            <a id="cancel-changes-button" class="button is-flexible is-danger is-stylish is-small is-hidden">
              <div class="icon is-small"><i class="fal fa-ban"></i></div>
              <span>Cancel</span>
            </a>
            <a id="status-toggle-button" tabindex="0" class="button is-flexible control-item is-primary is-inverted is-stylish is-small" title="Publicly hide or show listing page(s).">
              <div id="status-icon" class="icon is-small"><i class="fal fa-toggle-on"></i></div>
              <span id="status-text" class="is-hidden-mobile"></span>
            </a>
            <div id="view-listings-button-wrapper" class="is-inline control-item is-relative">
              <a id="view-listings-button" class="button is-flexible is-black is-inverted is-stylish is-small" target="_blank" rel="noopener" tabindex="0">
                <div class="icon is-small"><i class="fal fa-external-link"></i></div>
                <span class="is-hidden-mobile">View Listing<span class="edit-domain-plural is-hidden">s</span></span>
              </a>
              <div id="view-listings-button-drop" class="box domain-tooltip is-hidden"></div>
            </div>
          </div>

        </div>
      </div>
      <div class="tabs is-primary">
        <ul>
          <li id="info-tab" class="verified-elem unpurchased-elem tab">
            <a tabindex="0" class="">
              <span class="">Listing</span>
              <span class="is-hidden-mobile">&nbsp;Info</span>
            </a>
          </li>
          <li id="domain-info-tab" class="verified-elem unpurchased-elem tab">
            <a tabindex="0" class="">
              <span class="">Domain</span>
              <span class="is-hidden-mobile">&nbsp;Info</span>
            </a>
          </li>
          <li id="design-tab" class="verified-elem unpurchased-elem tab">
            <a tabindex="0" class="">
              <span class="is-hidden-mobile">Listing&nbsp;</span>
              <span class="">Design</span>
              <% if (!user.stripe_subscription_id) { %>
                <div class="icon is-small"><i class="fal fa-trophy-alt"></i></div>
              <% } %>
            </a>
          </li>
          <li id="hub-tab" class="verified-elem unpurchased-elem tab">
            <a tabindex="0" class="">
              <span class="">Hub</span>
              <% if (!user.stripe_subscription_id) { %>
                <div class="icon is-small"><i class="fal fa-trophy-alt"></i></div>
              <% } %>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div id="info-tab-drop" data-title="Edit Listing Information" class="tab-drop is-hidden">
      <div class="columns">
        <div class="column is-6">
          <div class="card is-fullwidth">
            <div class="card-header">
              <h3 class="card-header-title">Listing Details</h3>
            </div>
            <div class="card-content">
              <div class="control">
                <label class="label" for="description">
                  Listing Description
                </label>
                <small class="tip">Text description that will appear as a paragraph below the domain name on the listing page.</small>
                <textarea id="description" class="description-input textarea changeable-input"
                tabindex="0" data-name="description" placeholder="The greatest domain name to ever have existed. Buy it today and start investing in your next project or venture!"></textarea>
              </div>
              <div class="control">
                <label class="label" for="categories-input">
                  Categories
                </label>
                <small class="tip">Categories or industries <span class="this-domain">this domain</span> belong to. Helpful for users to find similar domains.</small>
                <input id="categories-input" class="input changeable-input" data-name="categories" type="text">
                <div id="categories-dropdown" class="is-hidden">
                  <%# make all the categories %>
                  <% for (var x = 0; x < categories.length; x++){ %>
                    <div class="category-checkbox-wrapper">
                      <input type="checkbox" id="<%= categories[x].back %>-category-input" class="category-checkbox-input" value="<%= categories[x].back %>" />
                      <label for="<%= categories[x].back %>-category-input"><%= categories[x].front %></label>
                    </div>
                  <% } %>
                </div>
              </div>
              <div class="control">
                <label class="label" for="description-footer">
                  <span>Footer Text</span>
                  <% if (!user.stripe_subscription_id) { %>
                    <div class="icon is-small is-tooltip" data-balloon="Premium Feature" data-balloon-pos="up">
                      <i class="fal fa-trophy-alt"></i>
                    </div>
                  <% } %>
                  <div class="icon is-small is-tooltip" data-balloon-length="medium" data-balloon="Maximum of 75 characters." data-balloon-pos="top">
                    <i class="fal fa-question-circle" data-fa-transform="down-3"></i>
                  </div>
                </label>
                <small class="tip">Text that is displayed on the footer of the listing page. An ideal location for a slogan, phrase, or trademark.</small>
                <input id="description-footer" data-name="description_footer" tabindex="0" class="premium-input input changeable-input" placeholder="The best domain names in the industry." />
              </div>
              <div class="control">
                <label class="label" for="description-footer-link">
                  <span>Footer URL</span>
                  <% if (!user.stripe_subscription_id) { %>
                    <div class="icon is-small is-tooltip" data-balloon="Premium Feature" data-balloon-pos="up">
                      <i class="fal fa-trophy-alt"></i>
                    </div>
                  <% } %>
                  <div class="icon is-small is-tooltip" data-balloon-length="medium" data-balloon="Leave empty for no links." data-balloon-pos="top">
                    <i class="fal fa-question-circle"></i>
                  </div>
                </label>
                <small class="tip">An optional link for your descriptive footer text and logo. Useful for linking to your listing hub.</small>
                <input id="description-footer-link" data-name="description_footer_link" tabindex="0" class="premium-input input changeable-input" placeholder="https://link-to-my-listing-hub.com" />
              </div>
            </div>
          </div>
        </div>
        <div class="column is-6">
          <div class="card is-fullwidth">
            <div class="card-header">
              <h3 class="card-header-title">
                Pricing
                <div class="icon is-danger is-small" data-balloon-length="large" data-balloon="We highly recommend that you turn your listing status to inactive before making any pricing changes." data-balloon-pos="up">
                  <i class="fal fa-exclamation-triangle"></i>
                </div>
              </h3>
            </div>
            <div class="card-content">
              <div class="control">
                <label class="label" for="min-price-input">
                  Default Currency
                  <div class="icon is-tooltip is-small" data-balloon="Please contact us if you do not see your currency listed." data-balloon-pos="up" data-balloon-length="large">
                    <i class="fal fa-question-circle"></i>
                  </div>
                </label>
                <small class="tip">The default currency for <span class="this-domain">this domain</span>. Overrides account-wide default currency.</small>
                <span class="select is-fullwidth">
                  <select required name="default_currency" id="default_currency-input" data-name="default_currency" class="changeable-input currency-select"></select>
                </span>
              </div>
              <div class="control">
                <div class="columns">
                  <div class="column is-6">
                    <div class="control">
                      <label class="label" for="min-price-input">
                        Minimum Price
                        <div class="icon is-small is-tooltip" data-balloon-length="large" data-balloon="Must be a whole number. Leave blank or set to 0 for no minimum price." data-balloon-pos="top">
                          <i class="fal fa-question-circle"></i>
                        </div>
                      </label>
                      <small class="tip">The absolute minimum price for <span class="this-domain">this domain</span>.</small>
                      <input id="min-price-input" class="input changeable-input" data-name="min_price" type="number" min="1" step="1">
                    </div>
                  </div>
                  <div class="column is-6">
                    <div class="control">
                      <label class="label" for="buy-price-input">
                        Buy It Now Price
                        <div class="icon is-small is-tooltip" data-balloon-length="large" data-balloon="Must be a whole number. Leave blank or set to 0 to disable the buy-it-now option." data-balloon-pos="top">
                          <i class="fal fa-question-circle"></i>
                        </div>
                      </label>
                      <small class="tip">The price to sell <span class="this-domain">this domain</span> for immediately.</small>
                      <input id="buy-price-input" class="input changeable-input" data-name="buy_price" type="number" min="1" step="1">
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="card is-fullwidth">
            <div class="card-header">
              <h3 class="card-header-title">Domain Rentals</h3>
              <a target="_blank" rel="noopener" href="/faq#renting" class="is-primary is-bold">Why rent my domain out?</a>
            </div>
            <div class="card-content">
              <div class="control is-spaced">
                <div class="control-left">
                  <label class="label" for="rentable-input">
                    Allow Rentals
                    <div class="icon is-small is-tooltip" data-balloon-length="large" data-balloon="A rented domain cannot be purchased during the rental term. For more details, please check the DomaHub FAQ." data-balloon-pos="up">
                      <i class="fal fa-question-circle" data-fa-transform="down-3"></i>
                    </div>
                  </label>
                  <small class="tip">Allows users to rent usage of <span class="this-domain">this domain</span> for short amounts of time.</small>
                </div>
                <div class="control-right">
                  <form class="checkbox-form">
                    <input id="rentable-input" data-name="rentable" type="checkbox" class="checkbox-input changeable-input" tabindex="0" />
                    <label for="rentable-input"></label>
                  </form>
                </div>
              </div>
              <label class="label">
                Rental Price
                <div class="icon is-small is-tooltip" data-balloon-length="large" data-balloon="Must be a whole number. Leave blank or set to 0 for free rentals on your domain." data-balloon-pos="up">
                  <i class="fal fa-question-circle" data-fa-transform="down-3"></i>
                </div>
              </label>
              <small class="tip">The price per unit of time for a domain rental.</small>
              <div class="control">
                <div class="control has-addons">
                  <input id="price-rate-input" data-name="price_rate" type="number" min="1" step="1" class="input changeable-input rentable-input" />
                  <span class="select is-stylish price-type-span no-shadow">
                    <select id="price-type-input" data-name="price_type" class="changeable-input rentable-input">
                      <option value="month">Month</option>
                      <option value="week">Week</option>
                      <option value="day">Day</option>
                    </select>
                  </span>
                </div>
              </div>
              <div class="control">
                <label class="label">
                  <span>Example Paths</span>
                  <div class="icon is-small is-tooltip" data-balloon-length="medium" data-balloon="Must consist of either letters or numbers." data-balloon-pos="up">
                    <i class="fal fa-question-circle" data-fa-transform="down-3"></i>
                  </div>
                </label>
                <small class="tip">Example paths or subdirectories that users may want to rent on <span class="this-domain">this domain</span>.</small>
                <input id="paths-input" data-name="paths" tabindex="0" class="is-hidden input rentable-input" />
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="domain-info-tab-drop" data-title="Edit Domain Information" class="tab-drop is-hidden">
      <div class="columns">
        <div class="column is-6">
          <div class="card is-fullwidth">
            <div class="card-header">
              <h3 class="card-header-title">
                Registrar Information
                <div class="icon is-danger is-small" data-balloon-length="large" data-balloon="Changing this information does not change it on your registrar." data-balloon-pos="up">
                  <i class="fal fa-exclamation-triangle"></i>
                </div>
              </h3>
              <a id="lookup-dns-button" class="button is-icon no-shadow is-stylish is-small">
                <span class="icon is-small"><i class="fal fa-search"></i></span>
                <span class="is-hidden-mobile">Lookup Missing Details</span>
              </a>
            </div>
            <div class="card-content">
              <div class="control">
                <div class="control">
                  <label class="label" for="registrar-name-input">
                    Registrar Name
                  </label>
                  <small class="tip">The registrar <span class="this-domain">this domain</span> is registered on.</small>
                  <input id="registrar-name-input" class="input changeable-input" data-name="registrar_name" type="text">
                </div>
              </div>
              <div class="control">
                <div class="control">
                  <label class="label" for="date-expire-input">
                    Domain Registration Date
                  </label>
                  <small class="tip">When <span class="this-domain">this domain</span> was first registered with its registrar.</small>
                  <input id="date-registered-input" class="input changeable-input" data-name="date_registered" type="datetime-local">
                </div>
              </div>
              <div class="control">
                <div class="control">
                  <label class="label" for="date-expire-input">
                    Domain Expiration Date
                  </label>
                  <small class="tip">When <span class="this-domain">this domain</span> will expire with its registrar.</small>
                  <input id="date-expire-input" class="input changeable-input" data-name="date_expire" type="datetime-local">
                </div>
              </div>
              <div class="control is-spaced">
                <div class="control-left">
                  <label class="label">
                    Edit Contact Details
                  </label>
                  <small class="tip">The various contact information for <span class="this-domain">this domain</span>.</small>
                </div>
                <div class="control-right has-text-right">
                  <span class="select no-shadow is-stylish">
                    <select id="edit-registrar-contact-button">
                      <option selected value="off" disabled>Click to edit contact details</option>
                      <option value="admin" data-text="Administrator">Edit Administrator</option>
                      <option value="registrant" data-text="Registrant">Edit Registrant</option>
                      <option value="tech" data-text="Tech">Edit Tech</option>
                    </select>
                  </span>
                </div>
              </div>
              <div class="control is-spaced">
                <div class="control-left">
                  <label class="label">
                    Administrator Details
                  </label>
                </div>
                <div class="control-right has-text-right">
                  <small id="registrar-admin_name-doesnt-exist" class="tip">No details exist!</small>
                  <small class="tip break-all"><span id="registrar-admin_name-text" ></span><span id="registrar-admin_org-text"></span></small>
                  <small class="tip break-all"><span id="registrar-admin_email-text" ></span><span id="registrar-admin_phone-text"></span></small>
                  <small class="tip break-all" id="registrar-admin_address-text"></small>
                </div>
              </div>
              <div class="control is-spaced">
                <div class="control-left">
                  <label class="label">
                    Registrant Details
                  </label>
                </div>
                <div class="control-right has-text-right">
                  <small id="registrar-registrant_name-doesnt-exist" class="tip">No details exist!</small>
                  <small class="tip break-all"><span id="registrar-registrant_name-text" ></span><span id="registrar-registrant_org-text"></span></small>
                  <small class="tip break-all"><span id="registrar-registrant_email-text" ></span><span id="registrar-registrant_phone-text"></span></small>
                  <small class="tip break-all" id="registrar-registrant_address-text"></small>
                </div>
              </div>
              <div class="control is-spaced">
                <div class="control-left">
                  <label class="label">
                    Tech Details
                  </label>
                </div>
                <div class="control-right has-text-right">
                  <small id="registrar-tech_name-doesnt-exist" class="tip">No details exist!</small>
                  <small class="tip break-all"><span id="registrar-tech_name-text" ></span><span id="registrar-tech_org-text"></span></small>
                  <small class="tip break-all"><span id="registrar-tech_email-text" ></span><span id="registrar-tech_phone-text"></span></small>
                  <small class="tip break-all" id="registrar-tech_address-text"></small>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="column is-6">
          <div class="card is-fullwidth">
            <div class="card-header is-block">
              <h3 class="card-header-title">
                Domain Expenses
              </h3>
              <small class="tip">Any costs, expenses, or invoices related to <span class="this-domain">this domain</span>.</small>
            </div>
            <div class="card-content">
              <div class="control">
                <label class="label" for="annual-cost-input">
                  Annual Renewal Cost
                  <div class="icon is-tooltip is-small" data-balloon="Please contact us if you do not see your currency listed." data-balloon-pos="up" data-balloon-length="large">
                    <i class="fal fa-question-circle"></i>
                  </div>
                </label>
                <small class="tip">How much it costs per year (in what currency) to renew <span class="this-domain">this domain</span> with your registrar.</small>
                <div class="control has-addons">
                  <span class="select is-fullwidth">
                    <select id="annual-cost-currency-input" required class="currency-select changeable-input" data-name="registrar_cost_currency"></select>
                  </span>
                  <input id="annual-cost-input" class="input changeable-input" data-name="registrar_cost" type="number" min="1" step="1">
                </div>
              </div>
              <div id="related-expenses-wrapper" class="remove-margin-bottom-content control is-spaced">
                <div class="control-left">
                  <label class="label">
                    Related Expenses
                  </label>
                  <small class="tip">Any expenses related to <span class="this-domain">this domain</span>. Such as fees, payments, or taxes.</small>
                </div>
                <div class="control-right has-text-right">
                  <a id="add-new-expense-button" tabindex="0" class='button no-shadow is-stylish is-small'>
                    <div class="icon is-small"><i class="fal fa-plus"></i></div>
                    <span>Add New Expense</span>
                  </a>
                </div>
              </div>
              <div id="domain-expense-table-wrapper" class="is-hidden control">

                <table class="table no-borders is-transparent-head no-shadow">
                  <tbody id="domain-expense-table">
                    <tr id="domain-expense-clone" class="clone-row is-hidden table-row no-hover-effect">
                      <td class="domain-expense-name break-word"></td>
                      <td class="domain-expense-cost"></td>
                      <td class="domain-expense-date"></td>
                      <td class="expense-button-wrapper control is-grouped is-pulled-right">
                        <a tabindex="0" class='domain-expense-delete-button button is-flexible no-shadow is-stylish is-small is-pulled-right'>
                          <div class="icon is-small"><i class="fal fa-trash"></i></div>
                          <span class="is-hidden-mobile">Delete</span>
                        </a>
                        <a tabindex="0" class='domain-expense-edit-button button is-flexible no-shadow is-stylish is-small is-pulled-right'>
                          <div class="icon is-small"><i class="fal fa-edit"></i></div>
                          <span class="is-hidden-mobile">Edit</span>
                        </a>
                      </td>
                    </tr>
                  </tbody>
                </table>

              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- REGISTRAR CONTACT MODAL -->
      <div id="registrar-contact-modal" class="modal">
        <div class="modal-background"></div>
        <div class="modal-container">
          <div class="modal-content">
            <div class="content has-text-centered">
              <h3 class="is-bold">Domain <span class="registrar-which-text"></span> Details
                <div class="icon is-danger is-small" data-balloon-length="medium" data-balloon="Changing this does not change it on your registrar." data-balloon-pos="left">
                  <i class="fal fa-exclamation-triangle"></i>
                </div>
              </h3>
            </div>
            <div class="content">
              <div class="control">
                <div class="control">
                  <label class="label" for="registrar-contact-name-input">
                    <span class="registrar-which-text"></span> Name
                  </label>
                  <input id="registrar-contact-name-input" class="registrar-modal-input input changeable-input" data-name="registrar_contact_name" type="text">
                </div>
              </div>
              <div class="control">
                <div class="control">
                  <label class="label" for="registrar-contact-org-input">
                    <span class="registrar-which-text"></span> Organization
                  </label>
                  <input id="registrar-contact-org-input" class="registrar-modal-input input" data-name="registrar_contact_org" type="text">
                </div>
              </div>
              <div class="control">
                <div class="control">
                  <label class="label" for="registrar-contact-email-input">
                    <span class="registrar-which-text"></span> Email
                  </label>
                  <input id="registrar-contact-email-input" class="registrar-modal-input input" data-name="registrar_contact_email" type="text">
                </div>
              </div>
              <div class="control">
                <div class="control">
                  <label class="label" for="registrar-contact-address-input">
                    <span class="registrar-which-text"></span> Address
                  </label>
                  <input id="registrar-contact-address-input" class="registrar-modal-input input" data-name="registrar_contact_address" type="text">
                </div>
              </div>
              <div class="control">
                <div class="control">
                  <label class="label" for="registrar-admin-phone-input">
                    <span class="registrar-which-text"></span> Phone
                  </label>
                  <input id="registrar-contact-phone-input" class="registrar-modal-input input" data-name="registrar_contact_phone" type="text">
                </div>
              </div>
              <div class="control is-grouped is-grouped-center">
                <div class="control is-expanded">
                  <a id="save-registrar-contact-button" tabindex="0" class='button is-fullwidth is-stylish is-small is-primary'>
                    <div class="icon is-small"><i class="fal fa-check"></i></div>
                    <span>Save Changes</span>
                  </a>
                </div>
                <div class="control is-expanded">
                  <a id="cancel-registrar-contact-button" tabindex="0" class='button is-fullwidth is-stylish is-small is-outlined is-danger'>
                    <div class="icon is-small"><i class="fal fa-times"></i></div>
                    <span>Cancel Changes</span>
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
        <button class="modal-close"></button>
      </div>

      <!-- DOMAIN EXPENSE MODAL -->
      <div id="domain-expense-modal" class="modal">
        <div class="modal-background"></div>
        <div class="modal-container">
          <div class="modal-content">
            <div class="content has-text-centered">
              <h3 class="is-bold"><span id="how-change-domain-expense"></span>Domain Expense</h3>
            </div>
            <div class="content">
              <form id="domain-expense-form">
                <div class="control">
                  <div class="control">
                    <label class="label" for="domain-expense-name-input">
                      Expense Name
                    </label>
                    <input required id="domain-expense-name-input" class="input domain-expense-input" data-name="domain_expense_name" type="text">
                  </div>
                </div>
                <div class="control">
                  <div class="control">
                    <label class="label" for="domain-expense-cost-input">
                      Expense Currency
                    </label>
                    <span class="select is-fullwidth">
                      <select id="domain-expense-currency-input" required data-name="domain_expense_currency" class="currency-select"></select>
                    </span>
                  </div>
                </div>
                <div class="control">
                  <div class="control">
                    <label class="label" for="domain-expense-cost-input">
                      Expense Cost
                    </label>
                    <input required id="domain-expense-cost-input" class="input domain-expense-input" data-name="domain_expense_cost" type="number" min="1" step=".01">
                  </div>
