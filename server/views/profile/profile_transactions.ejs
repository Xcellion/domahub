<div id="transactions" class="card is-fullwidth is-hidden" >
  <div class="card-header">
    <h2 class="card-header-title">
      <span>Transactions</span>
    </h2>
  </div>
  <div class="card-content">
    <table class="table">
      <thead>
        <tr>
          <th>Date</th>
          <th>Type</th>
          <th>Domain</th>
          <th class="has-text-right">Received</th>
          <th class="has-text-right">
            <a href="/faq#pricing" class="is-primary">
              <span>Fees Paid</span>
              <span class="icon is-small is-tooltip v-align-bottom" data-balloon-length="medium" data-balloon="Click to learn more about DomaHub fees." data-balloon-pos="up">
                <i class="fa fa-question-circle"></i>
              </span>
            </a>
          </th>
          <th class="has-text-right">Profit</th>
          <th></th>
        </tr>
      </thead>
      <tfoot>
        <tr>
          <td colspan="2"></td>
          <td class='is-bold'>Total</td>
          <td id="total-earnings" class='has-text-right is-bold'>$0.00</td>
          <td id="total-fees" class='has-text-right is-bold'>$0.00</td>
          <td id="total-profit" class='has-text-right is-bold'>$0.00</td>
          <td></td>
        </tr>
      </tfoot>
      <tbody class="earnings-table">
        <tr id="earnings-row-clone" class="earning-row is-hidden">
          <td class="earnings-row-date"></td>
          <td class="earnings-row-type"></td>
          <td class="earnings-row-domain"></td>
          <td class="earnings-row-amount has-text-right"></td>
          <td class="earnings-row-fees has-text-right"></td>
          <td class="earnings-row-profit has-text-right"></td>
          <td class="earnings-row-buttons">
            <button class="button earnings-row-refund-button no-shadow" tabindex="0">
              <span class="icon is-small">
                <i class="fa fa-undo"></i>
              </span>
              <span>Refund</span>
            </button>
          </td>
        </tr>
        <tr id="no-earnings-row">
          <td colspan="6">
            There are no transactions!
          </td>
        </tr>
      </tbody>
    </table>

